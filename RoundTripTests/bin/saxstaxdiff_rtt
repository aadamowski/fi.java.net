#!/bin/sh
echo -n $1 
xmltosaxtofi $1 > $1.sax.finf
xmltosaxtostaxtofi $1 > $1.stax.finf
diff $1.sax.finf $1.stax.finf > $1.sax.stax.diff
if [ -s $1.sax.stax.diff ]; then
	echo : FAILED
        report ${FIRTT_HOME} ${FIRTT_HOME}/report/report.html $1 $2 saxstaxdiff failed
else
	echo : PASSED
        report ${FIRTT_HOME} ${FIRTT_HOME}/report/report.html $1 $2 saxstaxdiff passed
fi
